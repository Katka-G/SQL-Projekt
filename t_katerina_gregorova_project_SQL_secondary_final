CREATE TABLE t_katerina_gregorova_project_SQL_secondary_final AS
	SELECT DISTINCT
		e.country,
		e.year,
		e.gdp,
		e.gini,
		e.population
	FROM 
		economies AS e
	JOIN 
		countries AS c
		ON e.country = c.country
	WHERE 
		e.gdp IS NOT NULL
		AND c.continent = 'Europe'
		AND e.YEAR >= 2006
		AND e.YEAR <= 2018
	ORDER BY
		e.country,
		e.year;
